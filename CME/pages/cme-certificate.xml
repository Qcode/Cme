<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatMessageDisplay" id="message_display" />
	<widget class="SwatForm" id="certificate_form">
		<widget class="SwatDisplayableContainer" id="article_bodytext_container">
			<widget class="SwatContentBlock" id="article_bodytext">
				<property name="content_type">text/xml</property>
			</widget>
		</widget>
		<widget class="SwatFormField" id="credits_field">
			<widget class="SwatCheckboxList" id="credits" />
		</widget>
		<widget class="SwatFooterFormField" id="submit_field">
			<widget class="SwatButton" id="submit_button">
				<property name="title" translatable="yes">Print Certificates</property>
			</widget>
		</widget>
	</widget>
	<widget class="SwatDisplayableContainer" id="cme_certificates">
		<widget class="SwatContentBlock" id="certificate">
			<property name="content_type">text/xml</property>
		</widget>
	</widget>
</swatml>
